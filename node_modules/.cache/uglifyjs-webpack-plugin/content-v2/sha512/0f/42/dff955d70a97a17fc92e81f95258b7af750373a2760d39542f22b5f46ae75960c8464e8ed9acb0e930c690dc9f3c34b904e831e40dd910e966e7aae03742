                               </a>\\n                            </div>\\n                        </div>\\n\\n                        {coin && coin.symbol ? (\\n                            <TransactionWrapper\\n                                asset={coin.symbol}\\n                                fromAccount={\\n                                    isDeposit\\n                                        ? coin.issuerId\\n                                        : this.props.account.get(\\\"id\\\")\\n                                }\\n                                to={\\n                                    isDeposit\\n                                        ? this.props.account.get(\\\"id\\\")\\n                                        : coin.issuerId\\n                                }\\n                            >\\n                                {({asset, to, fromAccount}) => {\\n                                    return (\\n                                        <RecentTransactions\\n                                            accountsList={Immutable.List([\\n                                                this.props.account.get(\\\"id\\\")\\n                                            ])}\\n                                            limit={10}\\n                                            compactView={true}\\n                                            fullHeight={true}\\n                                            filter=\\\"transfer\\\"\\n                                            title={\\n                                                <Translate\\n                                                    content={\\n                                                        \\\"gateway.recent_\\\" +\\n                                                        this.state.action\\n                                                    }\\n                                                />\\n                                            }\\n                                 