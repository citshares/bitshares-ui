\\n                );\\n                column = (\\n                    <span>\\n                        <TranslateWithLinks\\n                            string=\\\"operation.asset_issue\\\"\\n                            keys={[\\n                                {\\n                                    type: \\\"account\\\",\\n                                    value: op[1].issuer,\\n                                    arg: \\\"account\\\"\\n                                },\\n                                {\\n                                    type: \\\"amount\\\",\\n                                    value: op[1].asset_to_issue,\\n                                    arg: \\\"amount\\\"\\n                                },\\n                                {\\n                                    type: \\\"account\\\",\\n                                    value: op[1].issue_to_account,\\n                                    arg: \\\"to\\\"\\n                                }\\n                            ]}\\n                        />\\n                        {memoComponent}\\n                    </span>\\n                );\\n                break;\\n\\n            case \\\"asset_fund_fee_pool\\\":\\n                color = \\\"warning\\\";\\n\\n                column = (\\n                    <span>\\n                        <TranslateWithLinks\\n                            string=\\\"operation.asset_fund_fee_pool\\\"\\n                            keys={[\\n                                {\\n                                    type: \\\"account\\\",\\n                                    value: op[1].from_account,\\n                                    arg: \\\"account\\\"\\n                                },\\n                                {\\n                                    type: \\\"asset\\\",\\n                                    value: op[1].asset_id,\\n                                    arg: \\\"asset\\\"\\n                                },\\